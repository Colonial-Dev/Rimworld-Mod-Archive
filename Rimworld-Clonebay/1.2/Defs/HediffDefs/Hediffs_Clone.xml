<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef>
    <defName>QE_CloneStatus</defName>
    <label>Brain Scanned</label>
    <description>Brain Scanned</description>
    <defaultLabelColor>(0.2, 0.8, 0.2)</defaultLabelColor>
    <initialSeverity>1.0</initialSeverity>
    <stages>
      <li>
        <becomeVisible>true</becomeVisible>
      </li>
    </stages>
  </HediffDef>
  <HediffDef>
    <defName>QE_BrainTemplated</defName>
    <label>brain templated</label>
    <description>brain templated</description>
    <defaultLabelColor>(0.8, 0.2, 0.2)</defaultLabelColor>
    <initialSeverity>1.0</initialSeverity>
    <stages>
      <li>
        <becomeVisible>false</becomeVisible>
      </li>
    </stages>
  </HediffDef>
  <HediffDef>
    <defName>Clonebay_Implant</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Neural Scanner</label>
    <description>Neural Scanner</description>
    <labelNoun>neural scanner</labelNoun>
    <defaultLabelColor>(0.65, 0.3, 0.9)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <isBad>false</isBad>
  </HediffDef>
  <HediffDef>
    <defName>QE_Headache</defName>
    <label>headache</label>
    <description>headache</description>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.8, 0.8, 0.35)</defaultLabelColor>
    <initialSeverity>1.0</initialSeverity>
    <makesSickThought>true</makesSickThought>
    <comps>
      <li Class="HediffCompProperties_Immunizable">
        <severityPerDayNotImmune>-1.0</severityPerDayNotImmune>
        <severityPerDayImmune>-1.0</severityPerDayImmune>
      </li>
    </comps>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.10</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>